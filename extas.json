{
  "name": "deflou/core",
  "package_classes": [
    {
      "interface": "deflouActivityRepository",
      "class": "deflou\\components\\applications\\activities\\ActivityRepository"
    },
    {
      "interface": "deflouActivitySampleRepository",
      "class": "deflou\\components\\applications\\activities\\ActivitySampleRepository"
    },
    {
      "interface": "deflouAnchorRepository",
      "class": "deflou\\components\\applications\\anchors\\AnchorRepository"
    },
    {
      "interface": "deflouApplicationRepository",
      "class": "deflou\\components\\applications\\ApplicationRepository"
    },
    {
      "interface": "deflouApplicationSampleRepository",
      "class": "deflou\\components\\applications\\ApplicationSampleRepository"
    },
    {
      "interface": "deflouTriggerRepository",
      "class": "deflou\\components\\triggers\\TriggerRepository"
    },
    {
      "interface": "deflouTriggerSampleRepository",
      "class": "deflou\\components\\triggers\\TriggerSampleRepository"
    },
    {
      "interface": "deflouTriggerStateHistoryRepository",
      "class": "deflou\\components\\triggers\\TriggerStateHistoryRepository"
    },
    {
      "interface": "deflouTriggerResponseRepository",
      "class": "deflou\\components\\triggers\\TriggerResponseRepository"
    }
  ],
  "application_samples": [
    {
      "name": "deflou",
      "title": "DeFlou",
      "description": "DeFlou instance",
      "player_name": "admin",
      "parameters": {
        "schema": {
          "name": "schema",
          "title": "Schema",
          "description": "Schema"
        },
        "host": {
          "name": "host",
          "title": "Host",
          "description": "Host"
        },
        "port": {
          "name": "port",
          "title": "Port",
          "description": "Port"
        }
      },
      "created_at": "@timestamp(0)",
      "updated_at": "@timestamp(0)"
    }
  ],
  "application_activity_samples": [
    {
      "name": "event/nothing",
      "title": "Nothing",
      "description": "Nothing happened",
      "player_name": "admin",
      "type": "event",
      "class": "deflou\\components\\applications\\activities\\events\\EventNothing",
      "parameters": {},
      "created_at": "@timestamp(0)",
      "updated_at": "@timestamp(0)"
    },
    {
      "name": "action/nothing",
      "title": "Nothing",
      "description": "Nothing to do",
      "player_name": "admin",
      "type": "action",
      "class": "deflou\\components\\applications\\activities\\actions\\ActionNothing",
      "parameters": {},
      "created_at": "@timestamp(0)",
      "updated_at": "@timestamp(0)"
    }
  ],
  "plugins": [
    {
      "class": "extas\\components\\plugins\\repositories\\PluginFieldSampleName",
      "stage": [
        "extas.applications.create.before",
        "extas.activities.create.before",
        "extas.triggers.create.before",
        "extas.fields.create.before",
        "extas.fields_types.create.before"
      ]
    },
    {
      "class": "extas\\components\\plugins\\repositories\\PluginFieldUuid",
      "stage": [
        "extas.triggers_responses.create.before",
        "extas.anchors.create.before"
      ]
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallActivities",
      "stage": "extas.install.section.application_activities"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallActivitySamples",
      "stage": "extas.install.section.application_activity_samples"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallAnchors",
      "stage": "extas.install.section.application_anchors"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallApplications",
      "stage": "extas.install.section.applications"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallApplicationSamples",
      "stage": "extas.install.section.application_samples"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallTriggers",
      "stage": "extas.install.section.triggers"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallTriggerSamples",
      "stage": "extas.install.section.trigger_samples"
    },
    {
      "class": "deflou\\components\\plugins\\install\\InstallTriggerStateHistories",
      "stage": "extas.install.section.trigger_state_histories"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallActivities",
      "stage": "extas.uninstall.section.application_activities"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallActivitySamples",
      "stage": "extas.uninstall.section.application_activity_samples"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallAnchors",
      "stage": "extas.uninstall.section.application_anchors"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallApplications",
      "stage": "extas.uninstall.section.applications"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallApplicationSamples",
      "stage": "extas.uninstall.section.application_samples"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallTriggers",
      "stage": "extas.uninstall.section.triggers"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallTriggerSamples",
      "stage": "extas.uninstall.section.trigger_samples"
    },
    {
      "class": "deflou\\components\\plugins\\uninstall\\UninstallTriggerStateHistories",
      "stage": "extas.uninstall.section.trigger_state_histories"
    }
  ],
  "extensions": [
    {
      "class": "extas\\components\\extensions\\ExtensionHasCondition",
      "interface": "extas\\interfaces\\extensions\\IExtensionHasCondition",
      "methods": ["isConditionTrue", "getCondition", "getConditionName", "setConditionName"],
      "subject": "extas.sample.parameter"
    },
    {
      "class": "extas\\components\\extensions\\ExtensionRepositoryGet",
      "interface": "extas\\interfaces\\extensions\\IExtensionRepositoryGet",
      "subject": "*",
      "methods": [
        "deflouActivityRepository",
        "deflouActivitySampleRepository",
        "deflouAnchorRepository",
        "deflouApplicationRepository",
        "deflouApplicationSampleRepository",
        "deflouTriggerRepository",
        "deflouTriggerSampleRepository",
        "deflouTriggerStateHistoryRepository",
        "deflouTriggerResponseRepository"
      ]
    }
  ]
}